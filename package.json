{
  "name": "@hy66/element",
  "version": "1.0.5",
  "description": "Vue3 components library",
  "type": "module",
  "author": "hy",
  "license": "MIT",
  "keywords": ["Component", "UI", "Vue3"],
  "homepage": "http://element.vikingship.xyz/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hao-Cute/"
  },
  "files": [
    "dist"
  ],
  "sideEffects": ["dist/index.css"],
  "main": "./dist/umd/v-element.umd.cjs",
  "module": "./dist/es/v-element.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/v-element.js",
      "require": "./dist/umd/v-element.umd.cjs"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build-only && npm run move-style",
    "preview": "vite preview",
    "build-only": "run-p build-es build-umd",
    "build-umd": "vite build --config vite.umd.config.ts",
    "build-es": "vite build --config vite.es.config.ts",
    "move-style": "move-file dist/es/index.css dist/index.css",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@popperjs/core": "^2.11.6",
    "@vitepress-demo-preview/component": "^2.3.0",
    "@vitepress-demo-preview/plugin": "^1.4.0",
    "async-validator": "^4.2.5",
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "vue": "^3.5.20"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.6",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-each-variables": "^0.3.0",
    "postcss-for": "^2.1.1",
    "postcss-nested": "^7.0.2",
    "postcss-nesting": "^13.0.2",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitepress": "^1.6.4",
    "vitepress-theme-teek": "^1.4.5",
    "vue": "^3.5.20",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.8"
  }
}
